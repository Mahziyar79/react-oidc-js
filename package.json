{
  "name": "react-oidc-js",
  "version": "0.0.4",
  "description": "a wrapper for oidc-client-ts to work with react/react-native",
  "sideEffects": false,
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "prepare": "yarn build",
    "pretest": "yarn build",
    "build": "rm -rf ./lib && tsc",
    "test": "react-scripts test",
    "lint": "eslint src/**/*.ts",
    "eslint-fix": "eslint src/**/*.ts --fix",
    "prettier-fix": "prettier --write \"**/*.{ts,tsx}\"",
    "release": "auto shipit",
    "release-canary": "auto canary"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hosseinmd/react-oidc-js.git"
  },
  "keywords": [
    "react",
    "react-native",
    "react-native-web",
    "oidc-client",
    "oidc-client-ts",
    "identity server v4"
  ],
  "author": "Hossein mohammadi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hosseinmd/react-oidc-js/issues"
  },
  "homepage": "https://github.com/hosseinmd/react-oidc-js#readme",
  "devDependencies": {
    "@commitlint/config-conventional": "^8.3.4",
    "@types/jest": "^27.0.3",
    "@types/react": "^17.0.38",
    "@types/react-test-renderer": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.8.0",
    "commitlint": "^8.3.5",
    "eslint": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^4.2.3",
    "jest": "^27.4.5",
    "prettier": "^2.5.1",
    "prettier-plugin-jsdoc": "^0.3.30",
    "react": "^17.0.2",
    "react-scripts": "^5.0.0",
    "react-test-renderer": "^17.0.2",
    "typescript": "^4.5.4"
  },
  "peerDependencies": {
    "react": ">=16.12",
    "react-native": ">=0.61"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "yarn lint && yarn test"
    }
  },
  "files": [
    "lib",
    "src"
  ],
  "dependencies": {
    "jwt-decode": "^3.1.2",
    "oidc-client-ts": "^2.0.0-rc.3"
  }
}
