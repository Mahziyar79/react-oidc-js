{
  "name": "react-oidc-js",
  "version": "2.1.2",
  "description": "a wrapper for oidc-client-ts to work with react/react-native",
  "sideEffects": false,
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "prepare": "yarn build",
    "pretest": "yarn build",
    "build": "rimraf ./lib && tsc",
    "test": "react-scripts test",
    "lint": "eslint src/**/*.ts",
    "eslint-fix": "eslint src/**/*.ts --fix",
    "prettier-fix": "prettier --write \"**/*.{ts,tsx}\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hosseinmd/react-oidc-js.git"
  },
  "keywords": [
    "react",
    "react-native",
    "react-native-web",
    "oidc-client",
    "oidc-client-ts",
    "identity server v4"
  ],
  "author": "Hossein mohammadi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hosseinmd/react-oidc-js/issues"
  },
  "homepage": "https://github.com/hosseinmd/react-oidc-js#readme",
  "devDependencies": {
    "@commitlint/config-conventional": "^8.3.4",
    "@react-native-community/async-storage": "^1.12.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.3.0",
    "@types/jest": "^28.1.7",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "axios": "^0.25.0",
    "commitlint": "^8.3.5",
    "eslint": "^8.9.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^4.2.3",
    "jest": "^27.4.5",
    "prettier": "^2.5.1",
    "prettier-plugin-jsdoc": "^0.3.30",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "^5.0.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.7.4"
  },
  "peerDependencies": {
    "axios": ">=0.25",
    "react": ">=18",
    "react-native": ">=0.61"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "yarn lint && yarn test -- --watchAll=false"
    }
  },
  "files": [
    "lib",
    "src"
  ],
  "dependencies": {
    "@react-native-community/async-storage": "^1.12.1",
    "jwt-decode": "^3.1.2"
  }
}
